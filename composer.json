{
    "name": "laraning/galaxia",
    "description": "Galaxia - A Laravel admin framework that puts the universe in your hands.",
    "type": "library",
    "keywords": [
        "utilities",
        "laraning",
        "laravel",
        "php"
    ],
    "require": {
        "illuminate/support": "^5.6",
        "php": "^7.1",
        "laraning/flame": "*",
        "doctrine/dbal": "^2.7",
        "spatie/laravel-permission": "^2.17"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.1"
    },
    "autoload": {
        "psr-4": {
            "Laraning\\Galaxia\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Laraning\\GalaxiaTests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Laraning\\Galaxia\\GalaxiaServiceProvider",
                "Laraning\\Galaxia\\UserInterfaceProvider",
                "Laraning\\Galaxia\\EventServiceProvider"
            ]
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Galaxia",
            "email": "bruno.falcao@galaxia.laraning.com"
        }
    ],
    "minimum-stability": "dev"
}
