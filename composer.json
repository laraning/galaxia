{
    "name": "laraning/galaxia",
    "description": "Galaxia - A Laravel admin framework that puts the universe in your hands.",
    "type": "library",
    "keywords": [
        "utilities",
        "laraning",
        "laravel",
        "php"
    ],
    "require": {
        "illuminate/support": "^5.6",
        "php": "^7.1",
        "laraning/boost": "^1.0",
        "laraning/flame": "^1.0",
        "pragmarx/firewall": "^2.2",
        "doctrine/dbal": "^2.7",
        "spatie/laravel-permission": "^2.13"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.1"
    },
    "autoload": {
        "psr-4": {
            "Laraning\\Galaxia\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Laraning\\Galaxia\\Providers\\BaseServiceProvider",
                "Laraning\\Galaxia\\Providers\\UserInterfaceProvider"
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php artisan vendor:publish --provider=Laraning\\Galaxia\\Providers\\BaseServiceProvider",
            "@php artisan artisan vendor:publish --provider=\"Spatie\\Permission\\PermissionServiceProvider\" --tag=\"migrations\""
        ]
    },
    "config": {
        "preferred-install": "dev",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Laraning",
            "email": "bruno.falcao@laraning.com"
        }
    ],
    "minimum-stability": "dev"
}
